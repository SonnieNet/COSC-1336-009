# Assignment to variables
valueOne = 20
valueTwo = 30

# Find the multiple of numbers
result = valueOne * valueTwo

# Display result
print('\n')
print('\tSummary')
print('\tThe value 1 is : ', valueOne)
print('\tThe value 2 is : ', valueTwo)
print('\tThe product is: ', result)
